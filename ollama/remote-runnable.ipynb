   "source": [
    "from langserve import RemoteRunnable\n",
    "\n",
    "# 서버 주소 설정(localhost 기준)\n",
    "chain = RemoteRunnable(\"http://127.0.0.1:8000/prompt/\")\n",
    "\n",
    "# 체인 실행\n",
    "for token in chain.stream({\"topic\": \"딥러닝에 대해서 알려줘\"}):\n",
    "    print(token, end=\"\")"
   ]
